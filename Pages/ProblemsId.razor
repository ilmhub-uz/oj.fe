@inject HttpClient Http
@page "/problems{ProblemsId}"
@using Models.Problems
@using Newtonsoft.Json


@code {

    [Parameter]
    public ProblemsModel ProblemsModel { get; set; } = new() { Data = new() { ProblemModels = new()} };

    protected override async Task OnInitializedAsync()
    {
        var ProblemsId = await Http.GetFromJsonAsync<ProblemsModel>("http://oj.ilmhub.uz/api/problem?paging=true&offset=0&limit=10&page=1");
        Console.WriteLine($"{JsonConvert.SerializeObject(ProblemsId)}");
        StateHasChanged();
    }
}